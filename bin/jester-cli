#!/usr/bin/python

import socket
import re

s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
s.connect(('localhost', 8888))

input = ""
c = re.compile('source (.*)')

while True:
    input = raw_input("> ")
    if input == 'quit':
        break

    results = c.findall(input)    

    if results:
        print "Loading from " + results[0]
    else:
        s.send(input + "\n")
        result = s.recv(4096)
        print result






